   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Recourses/Rest.png" />
         </image>
      </ImageView>
      <Label layoutX="338.0" layoutY="158.0" text="Current Health: " textFill="WHITE" />
      <Label fx:id="health" layoutX="437.0" layoutY="158.0" text="0 / 0" textFill="WHITE" />
      <Button fx:id="back" layoutX="380.0" layoutY="549.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button fx:id="restoreHealth" layoutX="339.0" layoutY="102.0" mnemonicParsing="false" onAction="#restoreHealthpoints" text="Restore Health Now" />
